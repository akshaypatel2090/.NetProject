        <th>
            @Html.DisplayNameFor(model => model.skill_2)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.skill_3)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.programfast)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.graduationdate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.password)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.comment)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.alumni_id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.secondaryemail)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Campus)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.skill_1)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.skill_2)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.skill_3)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.programfast)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.graduationdate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.password)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.comment)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id = item.record_id }) |
                @Html.ActionLink("Details", "Details", new { id = item.record_id }) |
                @Html.ActionLink("Delete", "Delete", new { id = item.record_id })
            </td>
        </tr>
    }

</table>
